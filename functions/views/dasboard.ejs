  <center>
    
  <div class="col-md-10 mt-5 pl-10 pr-2" >
    <br>
      <hr>

      <!-- edit nama -->
      <div id="ubahnama">
          <h4>Edit Nama</h4>
         
            <div class="form-group">
              <div class="card bg-light" id="listnama">
                <% value.forEach(function(entry) {%>
                  <form action="" method="POST">
                  <div class="card-body" >
                    <div class="form-group">
                      <label>ID</label>
                      <input type="text" name="id_edit" class="form-control" value="<%=entry.id_edit%>" readonly>
                    </div>
                    <div class="form-group">
                      <label>Nama</label>
                      <input type="text" name="edit_nama" class="form-control" value="<%=entry.edit_nama%>">
                    </div>
                    <div class="form-group">
                      <label>Deskripsi</label>
                      <input type="text" name="edit_desc" class="form-control" value="<%=entry.edit_desc%>">
                    </div>
                    <div class="form-group">
                      <label>Gambar</label>
                      <input type="text" name="edit_gb" class="form-control" value="<%=entry.edit_gb%>">
                    </div>
                  </div>
                  <div class="form-group">
                    <button type="submit" class="btn btn-outline-dark" formaction="/update">Upload</button>

                  </div> 
         
                </form>
                  <%});%>
                </div>
            </div>

           
              
             

            <br>

      </div>
      <br>
      <hr>

     
  </div>
      



</div>
